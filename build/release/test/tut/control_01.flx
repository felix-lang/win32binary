proc f(x:int) 
{
  if x < 0 do
    println$ "Negative";
  elif x == 0 do
    println$ "Zero";
  else
    println$ "Positive";
  done
}
proc f(x:int) 
{
  while x > 0 do
    println$ x;
    x = x - 1;
  done
}  
proc f (n:int)
{
  for var i in 0 upto n - 1 do
    println$ i;
  done
  println$ i;
}
proc f(n:int)
{
  if n < 0 return;
  if n == 0 do
    println$ "Zero";
    return;
  done
  println$ "Positive";
  return;
}

proc f(n:int)
{
  if n < 0 goto endoff;
  println$ "Positive or Zero";
endoff:>
  println$ "Square is " + (n * n).str;
}
