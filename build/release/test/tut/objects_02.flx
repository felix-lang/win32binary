interface Person {
  get_name : 1 -> string;
  get_age : 1 -> int;
  set_age : int -> 0;
}
typedef Person2 = (
  get_name : 1 -> string,
  get_age : 1 -> int,
  set_age : int -> 0
);
object person (name:string, var age: int) implements Person = {
  method fun get_name () => name; 
  method fun get_age () => age; 
  method proc set_age (x:int) { age = x; }
};
interface WorkingPerson extends Person { 
  get_occupation: 1 -> string; 
}
