
@h1 struct_constructors-03.flx
@felix
// polymorphic struct constructors, without qualification

open module A {
  type a[t] = "?1";
  fun _ctor_a[t]: t -> a[t] = "$1";
  fun f[t]:a[t]->t="$1";
}

open module B {
  struct b[t] = { m: A::a[t]; };
  fun f[t](x:b[t])=>f (x.m);
}

var x = a(1.1);
var y = b(x);
println$ f y;

 

@expect
1.1
@
