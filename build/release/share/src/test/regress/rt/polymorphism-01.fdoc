
@h1 polymorphism-01.flx
@felix
//Check polymorphism

proc f[k] (z:k, pr:k->void) {
  fun diag[t] (x:t):t*t = { return x,x; }
  fun prj1[t1,t2] (x:t1,y:t2):t1 = { return x; }
  fun prj2[t1,t2] (x:t1,y:t2):t2 = { return y; }
  pr (prj1(diag z)); endl;
}

// eta expansion
proc pr(x:int) { fprint (cstdout,x); }
f[int] (1, pr);

@expect
1
